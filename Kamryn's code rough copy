        printf("Welcome");
        }else{ 
            printf("Incorrect password");
        }
        break;
        default:
        printf("You have entered an Incorrect password\n");
        }
        
        
    switch (Password){
        case 10:
        if (ID == 4321){
        printf("Welcome");
        }else{ 
            printf("Incorrect ID");
        }
        break;
        default:
        printf("You have entered an Incorrect ID\n");
        }
